{
  "name": "drone-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "ansi_up": "^4.0.4",
    "lodash.throttle": "^4.1.1",
    "portal-vue": "^2.1.7",
    "vue": "^2.6.12",
    "vue-click-outside": "^1.1.0",
    "vue-i18n": "^8.22.1",
    "vue-moment": "^4.1.0",
    "vue-router": "^3.4.7",
    "vuex": "^3.5.1",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.5.8",
    "@vue/cli-plugin-eslint": "^4.5.8",
    "@vue/cli-service": "^4.5.8",
    "@vue/eslint-config-prettier": "^6.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-vue": "^7.1.0",
    "node-sass": "^4.14.1",
    "sass-loader": "^10.0.4",
    "vue-template-compiler": "^2.6.12"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true,
      "es2021": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/prettier"
    ],
    "rules": {
      "vue/return-in-computed-property": "off",
      "vue/no-use-v-if-with-v-for": "off"
    },
    "parserOptions": {
      "sourceType": "module",
      "ecmaVersion": 2020,
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "last 2 Chrome versions"
  ],
  "engines": {
    "node": ">= 14.0.0"
  },
  "prettier": {
    "printWidth": 120
  }
}
